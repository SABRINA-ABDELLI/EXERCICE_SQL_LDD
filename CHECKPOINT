CREATE TABLE EMPLOYEES(
Num_Employee INT PRIMARY KEY,
 Name VARCHAR (255),
 Position VARCHAR (255),
 Salary DECIMAL(10,2),
 Num_dep INT FOREIGN KEY REFERENCES DEPARTMENT(Num_dep)

);
CREATE TABLE DEPARTMENT (
Num_dep INT PRIMARY KEY,
Label VARCHAR (255),
Manager_name VARCHAR (255)
);

CREATE TABLE PROJECTS(
Num_Pro INT PRIMARY KEY,
Title VARCHAR(255),
Startdate DATE,
End_date DATE,
Num_dep INT FOREIGN KEY REFERENCES DEPARTMENT (Num_dep));

CREATE TABLE EMPLOYEE_PROJECTS (
Num_Employee INT FOREIGN KEY REFERENCES EMPLOYEES (Num_Employee),
Num_Pro INT FOREIGN KEY REFERENCES PROJECTS(Num_Pro)
);



